    "rdfs:label" : "Principal Investigator"
  }, {
    "@id" : "ucdrp:GrantProgramDirectorRole",
    "@type" : "vivo:ResearcherRole",
    "ucdrp:abbrev" : "PDIR",
    "obo:IAO_0000112" : "Often used interchangeably with Principal Investigator (PD/PI); the person responsible for the project or program if awarded.",
    "rdfs:label" : "Program Director"
